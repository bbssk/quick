{
  "description": "Security profile of called application",
  "scopes": [
    {
      "name": "uaa.user",
      "description": "UAA"
    },
    {
      "name": "$XSAPPNAME.support",
      "description": "support"
    },
    {
      "name": "$XSAPPNAME.admin",
      "description": "admin"
    }
  ],
  "role-templates": [
    {
      "name": "Token_Exchange",
      "description": "UAA",
      "scope-references": [
        "uaa.user"
      ]
    },
    {
      "name": "support",
      "description": "generated",
      "scope-references": [
        "$XSAPPNAME.support"
      ],
      "attribute-references": []
    },
    {
      "name": "admin",
      "description": "generated",
      "scope-references": [
        "$XSAPPNAME.admin"
      ],
      "attribute-references": []
    }
  ],
  "attributes": [],
  "oauth2-configuration": {
    "credential-types": [
      "binding-secret",
      "x509"
    ],
    "redirect-uris": [
      "https://*.cfapps.us10.hana.ondemand.com/login/callback",
      "https://*.cfapps.us10-001.hana.ondemand.com/login/callback",
      "https://*.applicationstudio.cloud.sap/**",
      "https://*.cfapps.us10.hana.ondemand.com/login/callback",
      "http://localhost:5001/",
      "https://*.us10.trial.applicationstudio.cloud.sap/login/callback"
    ]
  }
}
